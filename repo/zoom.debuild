ZOOM__VERSION="6.7.0.6313"

zoom__version-pre() {
    echo "$ZOOM__VERSION"
}

zoom__get-sources() {
    curl -OL "https://cdn.zoom.us/prod/$ZOOM__VERSION/zoom_amd64.deb"
}

zoom__build() {
    destination="$1"
    cp *.deb "$destination/zoom_${ZOOM__VERSION}_amd64.deb"
}

zoom__version() {
    echo "$ZOOM__VERSION"
}
